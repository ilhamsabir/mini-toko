RewriteEngine On

RewriteCond $1 !^(index\.php)

RewriteRule ^wa/([^/\.]+)/?$ ./app/views/app.php?type=wa&slug=$1 [L]

RewriteRule ^sms/([^/\.]+)/?$ ./app/views/app.php?type=sms&slug=$1 [L]

RewriteRule ^bbm/([^/\.]+)/?$ ./app/views/app.php?type=bbm&slug=$1 [L]

RewriteRule ^telegram/([^/\.]+)/?$ ./app/views/app.php?type=telegram&slug=$1 [L]